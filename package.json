{
  "name": "@ts-tool/ts-codegen",
  "version": "0.7.0",
  "description": "Typescript code generator, which is able to convert swagger to ts code.",
  "main": "dist/index.js",
  "module": "module/index.js",
  "types": "src/index.ts",
  "bin": {
    "ts-codegen": "bin/ts-codegen.js"
  },
  "files": [
    "bin",
    "dist",
    "README.md"
  ],
  "scripts": {
    "start": "ts-node src/index.ts",
    "test": "jest",
    "build": "npm run build:es5 && npm run build:es6",
    "build:es5": "rm -rf dist/ && tsc",
    "build:es6": "rm -rf module && tsc --build ./tsconfig.build.es6.json",
    "prepare": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reeli/ts-codegen.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/reeli/ts-codegen/issues"
  },
  "homepage": "https://github.com/reeli/ts-codegen#readme",
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-typescript": "7.3.3",
    "@types/axios": "0.14.0",
    "@types/jest": "24.0.15",
    "@types/lodash": "4.14.136",
    "@types/node": "12.6.8",
    "@types/prettier": "1.18.0",
    "@types/swagger-schema-official": "2.0.18",
    "axios": "0.19.0",
    "babel-core": "6.26.3",
    "babel-jest": "24.8.0",
    "jest": "24.8.0",
    "prettier": "1.18.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "typescript": "3.5.3"
  },
  "dependencies": {
    "lodash": "4.17.15"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "testRegex": ".*/__tests__/.+\\.(generator|test|spec)\\.(ts|tsx)$",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/__types__/**",
      "!src/__tests__/**"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest",
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
